{
  "name": "flutter-generator",
  "version": "1.0.0",
  "description": "TypeScript-based Flutter app generator for StoreGo",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "generate": "ts-node src/index.ts --config config.json --output generated-app",
    "generate:fashion": "ts-node src/index.ts --config config.json --structure project-structure/fashion.json --output generated-app --template templates/fashion",
    "generate:custom": "ts-node src/index.ts",
    "scan:help": "ts-node scripts/ts/template-scanner.ts --help",
    "scan:fashion": "ts-node scripts/ts/template-scanner.ts --input src/templates --output project-structure/fashion.json",
    "check:struct": "ts-node scripts/ts/structure-checker.ts"
  },
  "keywords": [
    "flutter",
    "generator",
    "typescript"
  ],
  "author": "wassim",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.2",
    "handlebars": "^4.7.8"
  },
  "devDependencies": {
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "bin": {
    "flutter-generator": "./dist/index.js"
  }
}
